  urldate = {2015-01-07},
  date = {2013-08-01},
  pages = {245-268},
  keywords = {Plan of Chicago,STREETS,city practical,Edward Bennett,automobile,Daniel Burnham,city beautiful},
  author = {Kling, Samuel},
