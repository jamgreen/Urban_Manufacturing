  file = {C:\\Users\\elmue\\Zotero\\storage\\HGIC94GR\\Connelly et al. - 2011 - Bridging sustainability and the social economy Ac.pdf;C:\\Users\\elmue\\Zotero\\storage\\BC4QEE93\\Connelly et al. - 2011 - Bridging sustainability and the social economy Ac.html}
}

@article{GortonGovernmentSupportRegional2008,
  langid = {english},
  title = {Government {{Support}} to {{Regional Food Producers}}: {{An Assessment}} of {{England}}'s {{Regional Food Strategy}}},
