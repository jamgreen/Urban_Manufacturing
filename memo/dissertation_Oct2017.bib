@article{GrantTheoryPracticePlanning2009,
  title = {Theory and {{Practice}} in {{Planning}} the {{Suburbs}}: {{Challenges}} to {{Implementing New Urbanism}}, {{Smart Growth}}, and {{Sustainability Principles}}},
