@article{ParksUnevenGeographyRacial2012,
  title = {The {{Uneven Geography}} of {{Racial}} and {{Ethnic Wage Inequality}}: {{Specifying Local Labor Market Effects}}},
  volume = {102},
  issn = {0004-5608},
  url = {http://dx.doi.org/10.1080/00045608.2011.600198},
