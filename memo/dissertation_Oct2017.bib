  file = {C:\\Users\\elmue\\Zotero\\storage\\WMMEWR55\\Brinkley and Vitiello - 2014 - From Farm to Nuisance Animal Agriculture and the R.pdf;C:\\Users\\elmue\\Zotero\\storage\\69Z4M2JB\\113.html}
