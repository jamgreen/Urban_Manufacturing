@article{VallanceAlmostInvisibleGlimpsing2012,
  langid = {english},
  title = {Almost {{Invisible}}: {{Glimpsing}} the {{City}} and Its {{Residents}} in the {{Urban Sustainability Discourse}}},
  volume = {49},
  issn = {0042-0980, 1360-063X},
  url = {http://usj.sagepub.com/content/49/8/1695},
  doi = {10.1177/0042098011417903},
  shorttitle = {Almost {{Invisible}}},
  abstract = {‘Urban sustainability’ currently receives widespread and generally enthusiastic endorsement, yet concerns are emerging that recent expressions of the concept may actually be working against the city and its residents. Based on research in Christchurch, New Zealand (one of the most urbanised countries in the world), it is argued that the assimilation of social, economic and bio-physical environmental elements that gave the idea much of its original legitimacy has been reduced to a minimalist set of material and discursive ‘eco-friendly’ denominators. As a result, only occasional glimpses of the city and its human inhabitants are caught in attempts to operationalise sustainability in urban areas. The effect is that cities, in New Zealand at least, are less liveable and less likeable than they should be. It is suggested that there is a real need to re-urbanise and rehumanise the urban sustainability agenda as a means of realising its integrative and transformative potential.},
  number = {8},
  journaltitle = {Urban Studies},
  shortjournal = {Urban Stud},
  urldate = {2014-06-24},
  date = {2012-06-01},
  pages = {1695-1710},
  author = {Vallance, Suzanne and Perkins, Harvey C. and Bowring, Jacky and Dixon, Jennifer E.},
  file = {C:\\Users\\elmue\\Zotero\\storage\\57AI667C\\Vallance et al. - 2012 - Almost Invisible Glimpsing the City and its Resid.pdf;C:\\Users\\elmue\\Zotero\\storage\\DU7EPXQS\\Vallance et al. - 2012 - Almost Invisible Glimpsing the City and its Resid.pdf;C:\\Users\\elmue\\Zotero\\storage\\AK3D28TJ\\Vallance et al. - 2012 - Almost Invisible Glimpsing the City and its Resid.html;C:\\Users\\elmue\\Zotero\\storage\\NNTE2EJ8\\Vallance et al. - 2012 - Almost Invisible Glimpsing the City and its Resid.html}
