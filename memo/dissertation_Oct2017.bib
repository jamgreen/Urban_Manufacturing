  author = {McCloskey, Jon T. and Lilieholm, Robert J. and Cronan, Christopher},
  file = {C:\\Users\\elmue\\Zotero\\storage\\GI3TJ5FU\\McCloskey et al. - 2011 - Using Bayesian belief networks to identify potenti.pdf;C:\\Users\\elmue\\Zotero\\storage\\BXWUHHPB\\S0169204611000697.html}
