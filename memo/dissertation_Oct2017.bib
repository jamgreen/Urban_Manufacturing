@article{SandercockDigitalEthnographyPlanning2010,
  title = {Digital {{Ethnography}} as {{Planning Praxis}}: {{An Experiment}} with {{Film}} as {{Social Research}}, {{Community Engagement}} and {{Policy Dialogue}}},
